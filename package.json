    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
